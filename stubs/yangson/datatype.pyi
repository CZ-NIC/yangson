    def from_raw(self, raw: RawScalar) -> Optional[ScalarValue]: ...
    def to_raw(self, val: ScalarValue) ->  Optional[RawScalar]: ...
    def from_xml(self, xml: ET.Element) -> Optional[tuple[None]]: ...
    def to_xml(self, val: ScalarValue) -> Optional[str]: ...

